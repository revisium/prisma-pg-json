generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TestTable {
  id        String   @id @default(uuid())
  name      String
  data      Json
  meta      Json?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("test_tables")
}